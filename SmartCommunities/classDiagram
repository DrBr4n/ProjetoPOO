public class House
	private field id
	private field address
	private field ownerName
	private field nif
	private field deviceCounter
	private field devices
	private field rooms
	private field supplier
	public constructor House()
	public constructor House(String, String, String, String)
	public constructor House(String, String, String, String, Map<String, SmartDevice>, Map<String, Map<String, SmartDevice>>)
	public constructor House(House)
	public method getId()
	public method setId(String)
	public method getAddress()
	public method setAddress(String)
	public method getOwnerName()
	public method setOwnerName(String)
	public method getNif()
	public method setNif(String)
	public method getRooms()
	public method setRooms(Map<String, Map<String, SmartDevice>>)
	public method setSupplier(String)
	public method getSupplier()
	public method equals(Object)
	public method clone()
	public method toString()
	public method getDevices()
	public method existsDevice(String)
	public method turnDeviceOn(String)
	public method turnDeviceOff(String)
	public method turnAllOn()
	public method turnAllOff()
	public method existRoom(String)
	public method addRoom(String)
	public method addDeviceToRoom(String, SmartDevice)
	public method createDevice(String[])


